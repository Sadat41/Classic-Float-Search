{
  "manifest_version": 2,
  "name": "CSFloat Classic Search - Get rid of Spotlight",
  "version": "1.1.0",
  "description": "Restores the classic CSFloat search experience with sidebar input and autocomplete dropdown",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://csfloat.com/search*",
        "https://*.csfloat.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}